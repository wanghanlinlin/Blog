                                {
                                    "$ref": "#/definitions/res.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "Data": {
                                            "allOf": [
                                                {
                                                    "$ref": "#/definitions/plugins.PageResult"
                                                },
                                                {
                                                    "type": "object",
                                                    "properties": {
                                                        "Data": {
                                                            "type": "array",
                                                            "items": {
